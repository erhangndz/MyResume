@model UpdateContactDto
@{
    ViewData["Title"] = "Update";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<h3>İletişim Bilgisi Güncelle</h3>

<form method="post">
    <input type="hidden" asp-for="Id" />
    <label>Adres</label>
    <input type="text" asp-for="Address" class="form-control" />
    <span asp-validation-for="Address" class="text-danger"></span>
    <br />
    <label>Telefon</label>
    <input type="tel" asp-for="Phone" class="form-control" />
    <span asp-validation-for="Phone" class="text-danger"></span>
    <br />
    <label>Email</label>
    <input type="text" asp-for="Email" class="form-control" />
    <span asp-validation-for="Email" class="text-danger"></span>
    <br />
    <label>Instagram Url</label>
    <input type="text" asp-for="instagramUrl" class="form-control" />
    <span asp-validation-for="instagramUrl" class="text-danger"></span>
    <br />
    <label>Linkedin Url</label>
    <input type="text" asp-for="linkedinUrl" class="form-control" />
    <span asp-validation-for="linkedinUrl" class="text-danger"></span>
    <br />
    <label>Github Url</label>
    <input type="text" asp-for="githubUrl" class="form-control" />
    <span asp-validation-for="githubUrl" class="text-danger"></span>
    <br />
    <label>Youtube Url</label>
    <input type="text" asp-for="youtubeUrl" class="form-control" />
    <span asp-validation-for="youtubeUrl" class="text-danger"></span>
    <br />
    <label>Harita Embed Linki</label>
    <input type="text" asp-for="MapUrl" class="form-control" />
    <span asp-validation-for="MapUrl" class="text-danger"></span>
    <br />
    <button class="btn btn-primary"><i class="fas fa-pen"></i> Güncelle</button>

</form>