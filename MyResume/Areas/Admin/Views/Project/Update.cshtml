@model UpdateProjectDto
@{
    ViewData["Title"] = "Update";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<h3>Proje Güncelleme</h3>

<form method="post">
    <input type="hidden" asp-for="Id" />
    <label>Resim Url</label>
    <input type="text" asp-for="ImageUrl" class="form-control" />
    <br />
    <label>GitHub Linki</label>
    <input type="text" asp-for="GitHubUrl" class="form-control" />
    <br />
    <label>Proje Türü</label>
    <input type="text" asp-for="Type" class="form-control" />
    <br />
    <button class="btn btn-primary"><i class="fas fa-save"></i> Kaydet</button>
</form>